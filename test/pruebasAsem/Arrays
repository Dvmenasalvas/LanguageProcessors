###########Arrays de una dimensión
#Vacio
int[0] a1 = {};

#Lleno
int[5] a2 = { 1, 2, 3, 4, 5};

#Error de tipos
int[5] a3 = { 1, True, 3, 4, 5};

#Error de tamaño
int[5] a4 = { 1, 3, 4, 5};

#Dimension no entero
int x = 1;
int[x] a5;
int[2][x] a6;
int[True] a7;

##############Arrays de varias dimensiones
#Correcto
int[2] b1 = { 1, 2};
int[2] b2 = { 1, 2};
int[2][2] c1 = { b1, b2};

#Incorrecto por tamaño de dimension
int[3] b3 = { 1, 2, 3};
int[2][2] c2 = { b1, b3};

#Incorrecto por dimensiones
int[2][2] c3 = { b1, c1};

#Incorrecto por no ser array
int[2][2] c4 = { b1, 2};

#Incorrecto por ser array de otro tipo
boolean[2] b4 = { True, False};
int[2][2] c5 = { b1, b4};
